<?xml version="1.0"?>
<root>
  <OBJECT NAME="Contratos.Fechamentos">
    <METHOD NAME="ListaCooperadosFechamento" DESCRIPTION="" VERSION="9" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="Contrato" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Contrato" CTAB="0"/>
        <PARAM NAME="DataInicial" FORMAT="D" SIZE="10" PROJECTION="0" FIELDLABEL="Datainicial" CTAB="0"/>
        <PARAM NAME="DataFinal" FORMAT="D" SIZE="10" PROJECTION="0" FIELDLABEL="Datafinal" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="Cooperado" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Cooperado" CTAB="0"/>
        <FIELD NAME="Produto" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" FIELDLABEL="Produto" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>Select distinct cooperado, produto
from Apontamentos

where Contrato = :Contrato and
      DataEntrada between :DataInicial and :DataFinal
      and fechamento is null
order by cooperado

</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>