<?xml version="1.0"?>
<root>
  <OBJECT NAME="Feriados">
    <METHOD NAME="Listar" DESCRIPTION="Lista de Feriados" VERSION="7" INTFTYPE="5" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="Ano" SIZE="4" FLAGS="1" PROJECTION="0" FIELDLABEL="Ano" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="Id" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Id" CTAB="0"/>
        <FIELD NAME="Data" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" FIELDLABEL="Data" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT * FROM Feriados
WHERE [#YEAR(Data)=:Ano]
ORDER BY Data;
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>