<?xml version="1.0"?>
<root>
  <OBJECT NAME="Contratos">
    <METHOD NAME="Listar" DESCRIPTION="Lista de Contrato" VERSION="23" INTFTYPE="5" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="Contrato" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="N&#186; Contrato" CTAB="0"/>
        <PARAM NAME="Cliente" FORMAT="N" SIZE="8" FLAGS="1" STYLE="5" PROJECTION="0" ORDER="2" LOOKUP="CLIENTES.LISTAR" LOOKUPKEY="Id" LOOKUPDISPLAY="Nome" FIELDLABEL="Cliente" CTAB="0"/>
        <PARAM NAME="Cooperado" FORMAT="N" SIZE="8" FLAGS="1" STYLE="5" PROJECTION="0" ORDER="3" LOOKUP="COOPERADOS.LISTAR" LOOKUPKEY="Id" LOOKUPDISPLAY="Nome" LOOKUPCODE="Matricula" FIELDLABEL="Cooperado" CTAB="0"/>
        <PARAM NAME="Situacao" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="4" LOOKUP="list:'0','Vigente';'1','Finalizados';'3','Todos';" FIELDLABEL="Situa&#231;&#227;o" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="Id" FORMAT="N" SIZE="8" PROJECTION="0" ORDER="10" FIELDLABEL="Id" CTAB="0"/>
        <FIELD NAME="Descricao" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="Descri&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="Numero" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="6" FIELDLABEL="N&#186; Contrato" CTAB="0"/>
        <FIELD NAME="Cliente" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Cliente" CTAB="0"/>
        <FIELD NAME="DataInicio" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="9" FIELDLABEL="Data Inicio" CTAB="0"/>
        <FIELD NAME="DataTermino" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="10" FIELDLABEL="Data T&#233;rmino" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT
  CN.ID,
  CN.DESCRICAO,
  CN.NUMERO,
  CL.NOME AS CLIENTE,
  CN.DATAINICIO,
  CN.DATATERMINO
FROM CONTRATOS CN
INNER JOIN CLIENTES CL
ON CL.ID = CONTRATOS.CLIENTE
WHERE
  [CONTRATO=:CONTRATO AND]
  [CLIENTE=:CLIENTE AND]
  [COOPERADO=:COOPERADO] AND
  [SITUACAO=:SITUACAO]
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>