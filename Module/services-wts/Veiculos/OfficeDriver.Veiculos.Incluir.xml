          <GROUP/>
        </GROUPS>
        <FIELD NAME="Id" FORMAT="+" SIZE="8" FLAGS="1024" PROJECTION="0" FIELDLABEL="Id" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>INSERT INTO VEICULOS (FABRICANTE,
                      MODELO,
                      PLACA,
                      ANOFABRICACAO,
                      ANOMODELO,
                      TIPO,
                      POTENCIA,
                      TIPOCOMBUSTIVEL,
                      COR,
                      RASTREADOR,
                      NUMERORASTREADOR,
                      VENCIMENTOAPP,
                      TIPOPROPRIETARIO,
                      COOPERADO,
                      CLIENTE)
              VALUES (:FABRICANTE,
                      :MODELO,
                      :PLACA,
                      :ANOFABRICACAO,
                      :ANOMODELO,
                      :TIPO,
                      :POTENCIA,
                      :TIPOCOMBUSTIVEL,
                      :COR,
                      :RASTREADOR,
                      :NUMERORASTREADOR,
                      :VENCIMENTOAPP,
                      :TIPOPROPRIETARIO,
                      :COOPERADO,
                      :CLIENTE)
#RETURN(ID);
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
