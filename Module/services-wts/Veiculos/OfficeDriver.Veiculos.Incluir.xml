<?xml version="1.0"?>
<root>
  <OBJECT NAME="Veiculos">
    <METHOD NAME="Incluir" DESCRIPTION="" VERSION="26" INTFTYPE="1" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="Fabricante" FORMAT="N" SIZE="8" FLAGS="17" PROJECTION="0" ORDER="1" LOOKUP="OFFICEDRIVER.FABRICANTES.LISTAR" LOOKUPKEY="ID" LOOKUPDISPLAY="Descricao" FIELDLABEL="Fabricante" CTAB="0"/>
        <PARAM NAME="Modelo" SIZE="100" FLAGS="17" PROJECTION="0" ORDER="2" FIELDLABEL="Modelo" CTAB="0"/>
        <PARAM NAME="Placa" SIZE="8" FLAGS="17" PROJECTION="0" ORDER="6" FIELDLABEL="Placa" CTAB="0"/>
        <PARAM NAME="AnoFabricacao" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Ano Fabrica&#231;&#227;o" CTAB="0"/>
        <PARAM NAME="AnoModelo" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Ano Modelo" CTAB="0"/>
        <PARAM NAME="Tipo" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="10" LOOKUP="list:'H','Hatchback';'S','Sedan';'P','Pick-up';'V','Van';" FIELDLABEL="Tipo de Ve&#237;culo" CTAB="0"/>
        <PARAM NAME="Potencia" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="Potencia" CTAB="0"/>
        <PARAM NAME="TipoCombustivel" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="5" LOOKUP="list:'G','Gasolina';'A','Alcool';'D','Diesel';'F','Flex';'E','El&#233;trico';" FIELDLABEL="Tipo de Combust&#237;vel" CTAB="0"/>
        <PARAM NAME="Cor" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="11" FIELDLABEL="Cor" CTAB="0"/>
        <PARAM NAME="Rastreador" SIZE="40" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Rastreador" CTAB="0"/>
        <PARAM NAME="NumeroRastreador" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="9" FIELDLABEL="N&#186; Rastreador" CTAB="0"/>
        <PARAM NAME="VencimentoAPP" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="12" FIELDLABEL="Vencimento APP" CTAB="0"/>
        <PARAM NAME="TipoProprietario" FORMAT="N" SIZE="8" FLAGS="1" DEFAULT="1" PROJECTION="0" ORDER="13" LOOKUP="list:'1','Cooperado';'2','Cliente';'3','Cooperativa';" FIELDLABEL="Tipo Propriet&#225;rio" CTAB="0"/>
