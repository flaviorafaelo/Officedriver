<?xml version="1.0"?>
<root>
  <OBJECT NAME="Veiculos">
    <METHOD NAME="Listar" DESCRIPTION="" VERSION="7" INTFTYPE="5" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="Modelo" SIZE="100" FLAGS="1" PROJECTION="0" FIELDLABEL="Modelo" CTAB="0"/>
        <PARAM NAME="AnoModelo" FORMAT="N" SIZE="8" FLAGS="1" PROJECTION="0" FIELDLABEL="Anomodelo" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="Id" FORMAT="+" SIZE="8" FLAGS="1024" PROJECTION="0" FIELDLABEL="Id" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT MODELO, FABRINCANTE, ANOMODELO, ANOFABRICACAO
FROM MODELO M INNER JOIN  FABRINCANTE F
ON F.FABRINCANTE = M.FABRINCANTE
WHERE [M.MODELO LIKE :MODELO]
[AND M.ANOMODELO = :ANOMODELO]
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>