<?xml version="1.0"?>
<root>
  <OBJECT NAME="Cooperados">
    <METHOD NAME="Consultar" DESCRIPTION="Consultar" VERSION="76" INTFTYPE="6" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="Id" FORMAT="+" SIZE="8" FLAGS="1024" PROJECTION="0" FIELDLABEL="Id" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
          <GROUP NAME="Dados Pagamento"/>
          <GROUP NAME="Telefones"/>
          <GROUP NAME="Endere&#231;o"/>
          <GROUP NAME="Contatos"/>
          <GROUP NAME="Documentos" STYLE="1"/>
          <GROUP NAME="Rg"/>
          <GROUP NAME="Carteira Nacional de Habilita&#231;&#227;o"/>
        </GROUPS>
        <FIELD NAME="Id" FORMAT="+" SIZE="8" FLAGS="1024" PROJECTION="0" FIELDLABEL="Id" CTAB="0"/>
        <FIELD NAME="Matricula" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Matr&#237;cula" CTAB="0"/>
        <FIELD NAME="Foto" FORMAT="I" FLAGS="1" PROJECTION="0" ORDER="10" FIELDLABEL="Foto" CTAB="0"/>
        <FIELD NAME="Nome" SIZE="200" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Nome" CTAB="0"/>
        <FIELD NAME="Nascimento" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="8" FIELDLABEL="Nascimento" CTAB="0"/>
        <FIELD NAME="Rg" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="25" FIELDLABEL="Rg" GROUPNAME="Rg" CTAB="0"/>
        <FIELD NAME="RgExpedicao" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="26" FIELDLABEL="Expedi&#231;&#227;o" GROUPNAME="Rg" CTAB="0"/>
        <FIELD NAME="Cpf" SIZE="14" FLAGS="1" PROJECTION="0" ORDER="23" FIELDLABEL="Cpf" GROUPNAME="Documentos" CTAB="0"/>
        <FIELD NAME="Cnh" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="28" FIELDLABEL="N&#250;mero" GROUPNAME="Carteira Nacional de Habilita&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="CnhValidade" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="29" FIELDLABEL="Validade" GROUPNAME="Carteira Nacional de Habilita&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="CnhCategoria" SIZE="1" FLAGS="1" PROJECTION="0" ORDER="30" LOOKUP="list:'A','A';'B','B';'C','C';'D','D';'E','E';" FIELDLABEL="Categoria" GROUPNAME="Carteira Nacional de Habilita&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="FreeLancer" FORMAT="B" SIZE="1" FLAGS="17" DEFAULT="false" PROJECTION="0" ORDER="9" FIELDLABEL="Freelancer" CTAB="0"/>
        <FIELD NAME="Endereco" FORMAT="R" FLAGS="1" STYLE="8" PROJECTION="0" ORDER="19" FIELDLABEL="Endere&#231;o" NESTEDNAME="officedriver.Pessoas.Endereco" GROUPNAME="Endere&#231;o" CTAB="0"/>
        <FIELD NAME="DataMatricula" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="4" FIELDLABEL="Data Matr&#237;cula" CTAB="0"/>
        <FIELD NAME="DataCadastro" FORMAT="D" SIZE="10" FLAGS="1" DEFAULT="Today" PROJECTION="0" ORDER="3" FIELDLABEL="Data Cadastro" CTAB="0"/>
        <FIELD NAME="DataDesligamento" FORMAT="D" SIZE="10" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="Data Desligamento" CTAB="0"/>
        <FIELD NAME="Situacao" FORMAT="N" SIZE="8" FLAGS="1" DEFAULT="1" PROJECTION="0" ORDER="6" LOOKUP="list:'0','Desligado';'1','Ativo';" FIELDLABEL="Situa&#231;&#227;o" CTAB="0"/>
        <FIELD NAME="Contatos" FORMAT="R" FLAGS="1" PROJECTION="0" ORDER="21" FIELDLABEL="Contatos" NESTEDNAME="officedriver.Pessoas.Contato" GROUPNAME="Contatos" CTAB="0"/>
        <FIELD NAME="Pagamento" FORMAT="R" FLAGS="1" STYLE="8" PROJECTION="0" ORDER="12" FIELDLABEL="Pagamento" NESTEDNAME="OFFICEDRIVER.COOPERADOS.PAGAMENTO" GROUPNAME="Dados Pagamento" CTAB="0"/>
        <FIELD NAME="DddResidencial" SIZE="5" FLAGS="1" PROJECTION="0" ORDER="14" FIELDLABEL="DDD Resid&#234;ncial" GROUPNAME="Telefones" CTAB="0"/>
        <FIELD NAME="TelefoneResidencial" SIZE="10" FLAGS="1" MASK="####-####" PROJECTION="0" ORDER="15" FIELDLABEL="Telefone Resid&#234;ncial" GROUPNAME="Telefones" CTAB="0"/>
        <FIELD NAME="DddCelular" SIZE="5" FLAGS="1" PROJECTION="0" ORDER="16" FIELDLABEL="DDD Celular" GROUPNAME="Telefones" CTAB="0"/>
        <FIELD NAME="TelefoneCelular" SIZE="10" FLAGS="1" MASK="#####-####" PROJECTION="0" ORDER="17" FIELDLABEL="Telefone Celular" GROUPNAME="Telefones" CTAB="0"/>
        <FIELD NAME="Email" SIZE="255" FLAGS="1" PROJECTION="0" ORDER="7" FIELDLABEL="Email" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT ID,
       MATRICULA,
       FOTO,
       NOME,
       NASCIMENTO,
       RG,
       RGEXPEDICAO,
       CPF,
       CNH,
       CNHVALIDADE,
       CNHCATEGORIA,
       FREELANCER,
       DATAMATRICULA,
       DATACADASTRO,
       DATADESLIGAMENTO,
       SITUACAO,
       DDDRESIDENCIAL,
       TELEFONERESIDENCIAL,
       DDDCELULAR,
       TELEFONECELULAR,
       EMAIL
       
       #ROWSET({SELECT:PAGAMENTO
                       PAGAMENTOTIPO AS TIPO,
                       PAGAMENTOBANCO AS BANCO,
                       PAGAMENTOTIPOCONTA AS TIPOCONTA,
                       PAGAMENTOAGENCIA AS AGENCIA,
                       PAGAMENTONUMEROCONTA AS NUMEROCONTA
               FROM COOPERADOS
               WHERE ID=:ID})

       #ROWSET({SELECT:ENDERECO
                      TIPOPESSOA,
                      PESSOA,
                      CEP,
                      LOGRADOURO,
                      NUMERO,
                      COMPLEMENTO,
                      BAIRRO,
                      ESTADO,
                      CIDADE
                FROM ENDERECOS
                WHERE TIPOPESSOA = 0 AND
                      PESSOA=:ID})
                      
       #ROWSET({SELECT:CONTATOS
                      TIPOPESSOA,
                      PESSOA,
                      NOME,
                      TIPO,
                      DDD,
                      TELEFONE,
                      EMAIL
                FROM CONTATOS
                WHERE TIPOPESSOA = 0 AND
                      PESSOA=:ID})
FROM COOPERADOS
WHERE ID=:ID;
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>