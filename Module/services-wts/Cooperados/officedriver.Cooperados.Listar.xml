<?xml version="1.0"?>
<root>
  <OBJECT NAME="Cooperados">
    <METHOD NAME="Listar" DESCRIPTION="Lista de Cooperados" VERSION="19" INTFTYPE="5" THREADSAFE="0">
      <PARAMS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <PARAM NAME="Matricula" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="1" FIELDLABEL="Codigo" CTAB="0"/>
        <PARAM NAME="Nome" SIZE="200" FLAGS="1" PROJECTION="0" ORDER="2" FIELDLABEL="Nome" CTAB="0"/>
        <PARAM NAME="Contrato" FLAGS="1" PROJECTION="0" ORDER="3" FIELDLABEL="Contrato" CTAB="0"/>
      </PARAMS>
      <FIELDS>
        <GROUPS>
          <GROUP/>
        </GROUPS>
        <FIELD NAME="Id" FORMAT="N" SIZE="8" PROJECTION="0" FIELDLABEL="Id" CTAB="0"/>
        <FIELD NAME="Matricula" SIZE="20" FLAGS="1" PROJECTION="0" ORDER="5" FIELDLABEL="Matr&#237;cula" CTAB="0"/>
        <FIELD NAME="Nome" SIZE="100" FLAGS="1" PROJECTION="0" ORDER="6" FIELDLABEL="Nome" CTAB="0"/>
      </FIELDS>
      <ACTIONSCRIPT>SELECT ID,
       MATRICULA,
       NOME
FROM COOPERADOS
WHERE [MATRICULA=:MATRICULA]
      [NOME LIKE %:NOME%]
ORDER BY NOME
</ACTIONSCRIPT>
    </METHOD>
  </OBJECT>
</root>