<?xml version="1.0"?>
<root>
	<dimension>
    
		<tables>
			<table name="Contratos"/>
		</tables>
		
		<outriggers>

			<outrigger fullname="clientes$clientes">
				<relation cardinality="N-0">
					<condition  left="contratos.cliente" right="clientes$clientes.id"/>
				</relation>
			</outrigger>	
		
		</outriggers>			
		
		<attributes name="contratos">
			<level>
				<attribute name="Id"/>
				<attribute name="Numero"/>
				<attribute name="Descricao"/>
				<attribute name="Tipo" caption="Veiculo"/>
				<attribute name="Cliente" caption="DataEntrada"/>
				<attribute name="DataInicio" caption="HoraEntrada"/>
				<attribute name="DataTermino" caption="KmInicial"/>
				<attribute name="DiaFechamento" caption="DataSaida"/>
				<attribute name="IntervaloDiaCooperado" caption="HoraSaida"/>
				<attribute name="Valor" caption="KmFinal"/>
				<attribute name="AdiantamentoProducao" caption="IntervaloDescanso"/>
				<attribute name="BaseCalculo" caption="Viagem"/>
				<attribute name="MinimoHoras" caption="Observacao"/>		
			</level>
		</attributes>
		
	</dimension>
</root>
